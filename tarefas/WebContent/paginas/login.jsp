<div class="col-xl-3 col-lg-5 col-md-10 col-sm-10">
	<div class="card mb-2">
		<h5 class="card-header temaclaro text-light">Login</h5>
		<div class="card-body text-justify">
			<form action="./login" method="post">
				<div class="form-group">
					<label for="email">E-mail</label> <input type="text" class="form-control" name="email">
				</div>
				<div class="form-group">
					<label for="senha">Senha</label> <input type="password" class="form-control" name="senha">
				</div>
				<button type="submit" class="btn btn-colorido">Login</button>
				<button type="button" class="btn btn-outline-colorido" data-target="#modalCadastro" data-toggle="modal">
					Fazer cadastro
				</button>
			</form>
		</div>
	</div>
</div>